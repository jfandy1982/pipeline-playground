{
  "name": "issue",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "todo",
  "sourceRoot": "apps/issue/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:application",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/issue",
        "index": "apps/issue/src/index.html",
        "browser": "apps/issue/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/issue/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": ["apps/issue/src/favicon.ico", "apps/issue/src/assets"],
        "styles": ["apps/issue/src/styles.scss"],
        "scripts": [],
        "server": "apps/issue/src/main.server.ts",
        "prerender": true,
        "ssr": {
          "entry": "apps/issue/server.ts"
        }
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "options": {
        "port": 4200
      },
      "configurations": {
        "production": {
          "buildTarget": "issue:build:production"
        },
        "development": {
          "buildTarget": "issue:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "issue:build"
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "issue:build",
        "port": 4200,
        "staticFilePath": "dist/apps/issue/browser"
      }
    }
  }
}
